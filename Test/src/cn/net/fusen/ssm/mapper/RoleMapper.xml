<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.net.fusen.ssm.mapper.RoleMapper" >
	<!-- 添加角色 -->
	<insert id="addRole" parameterType="role">
		insert into sysrole(rolename,rolecategory,roledesc) values(#{rolename},#{rolecategory},#{roledesc})
	</insert>
	
	<!-- 角色列表 -->
	<!-- <select id="roleList" parameterType="hashmap" resultType="sysrole">
		select id from orguser where userid=#{userid}
	</select> -->
</mapper>